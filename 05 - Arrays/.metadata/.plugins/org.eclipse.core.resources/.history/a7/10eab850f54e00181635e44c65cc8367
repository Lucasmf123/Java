package exerciciobonus;

import java.util.Random;

public class ExBônus {

	public static void main(String[] args) {

		// Vetores
		int[] dados = new int[5];
		Random r = new Random();

		// Laço
		for (int indice = 0; indice < 4; indice++) {
			dados[indice] = (r.nextInt(21));
		}

		for (int i = 0; i < 4; i++) {
			System.out.println(dados[i]);
		}

	}

}
