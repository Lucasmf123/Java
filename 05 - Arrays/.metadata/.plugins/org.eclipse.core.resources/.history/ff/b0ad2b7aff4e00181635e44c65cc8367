package exerciciobonus;

import java.util.Random;

import javax.swing.JOptionPane;

public class ExBônus {

	public static void main(String[] args) {

		// Vetores
		int[] numero = new int[5];
		int pegarNumero = 0;
		Random r = new Random();
		boolean valida;

		// Adicionar dados ao vetor
		for (int i = 0; i < 5; i++) {

			// Obter o número
			do {
				valida = false;

				pegarNumero = (r.nextInt(21));

				// Validar se o número existe
				for (int i2 = 0; i2 < 5; i2++) {

					if (pegarNumero == numero[i2]) {
						valida = true;
						break;
					}
				}

				// Adicionar no vetor
				if (valida != true) {

					for (int i2 = 0; i2 < 5; i2++) {
						if (numero[i2] == 0) {
							numero[i2] = pegarNumero;
							break;
						}
					}

				} else {
					JOptionPane.showMessageDialog(null, "Número já informado!");
				}

			} while (valida != false);

		}

	}

}
